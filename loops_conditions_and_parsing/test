#!/bin/bash

# Initialize a counter variable
counter=1

# Loop while the counter is less than or equal to 10
while [ $counter -le 10 ]
do
    # Check if it's the 9th iteration
    if [ $counter -eq 9 ]
    then
        # Display "Best School" and "Hi" on separate lines
        echo "Best School"
        echo "Hi"
    else
        # Display "Best School" for other iterations
        echo "Best School"
    fi
    
    # Increment the counter
    ((counter++))
done

